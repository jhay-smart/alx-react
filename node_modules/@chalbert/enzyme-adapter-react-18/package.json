{
  "name": "@chalbert/enzyme-adapter-react-18",
  "version": "0.7.2",
  "description": "JavaScript Testing utilities for React",
  "homepage": "https://enzymejs.github.io/enzyme/",
  "main": "build",
  "workspaces": [
    "test"
  ],
  "scripts": {
    "build": "babel --source-maps=both src -d build",
    "clean": "rimraf build",
    "lint": "eslint src/ --ext .jsx,.js",
    "prepack": "yarn clean && yarn build",
    "test": "yarn lint"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "contributors": [
    "Wojciech Maj <kontakt@wojtekmaj.pl>",
    "Nicolas Gilbert <nic.g.gilbert@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@chalbert/enzyme-adapter-utils": "^0.1.0",
    "enzyme-shallow-equal": "^1.0.4",
    "has": "^1.0.3",
    "prop-types": "^15.8.1",
    "react-is": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "husky": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.16.0",
    "eslint-config-wojtekmaj": "^0.6.6",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "enzyme": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "resolutions": {
    "semver@7.0.0": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "src",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chalbert/enzyme-adapter-react-18"
  },
  "funding": "https://github.com/wojtekmaj/enzyme-adapter-react-17?sponsor=1",
  "packageManager": "yarn@3.1.0"
