{
  "name": "@chalbert/enzyme-adapter-utils",
  "version": "0.1.2",
  "description": "JavaScript Testing utilities for React",
  "homepage": "https://enzymejs.github.io/enzyme/",
  "main": "build",
  "scripts": {
    "build": "babel --source-maps=both src -d build",
    "clean": "rimraf build",
    "lint": "eslint --ext js,jsx .",
    "prepack": "yarn clean && yarn build",
    "test": "yarn lint",
    "watch": "yarn build -w"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "contributors": [
    "Wojciech Maj <kontakt@wojtekmaj.pl>",
    "Nicolas Gilbert <nic.g.gilbert@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "function.prototype.name": "^1.1.5",
    "has": "^1.0.3",
    "object.fromentries": "^2.0.5",
    "prop-types": "^15.8.1",
    "husky": "^7.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "eslint": "^8.16.0",
    "eslint-config-wojtekmaj": "^0.6.6",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0-0"
  },
  "resolutions": {
    "semver@7.0.0": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chalbert/enzyme-adapter-utils.git"
  },
  "funding": "https://github.com/wojtekmaj/enzyme-adapter-utils?sponsor=1",
  "packageManager": "yarn@3.1.0"
}
